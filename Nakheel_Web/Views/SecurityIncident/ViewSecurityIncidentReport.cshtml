    @*/*ACTION TAKEN ADD PAGE*/*@
    <form id="AddModule_Taken" action="#">
        <div class="modal fade add-new" tabindex="-1" role="dialog" aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="myExtraLargeModalLabel">Action Taken</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="col-md-12">
                                    <label for="example-search-input" class="form-model" style="color:black;">Action Taken Description<span class="requ">*</span></label>
                                    <textarea class="form-control" rows="1" cols="40" id="Sec_Action_Taken" name="Sec_Action_Taken" required minlen="1"></textarea>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="col-md-12">
                                    <label for="example-search-input" class="form-model" style="color:black;">Comments<span class="requ">*</span></label>
                                    <textarea class="form-control" rows="1" cols="40" id="Sec_Comments" name="Sec_Comments" required minlen="1"></textarea>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="col-md-12">
                                    <label for="example-search-input" class="form-model" style="color:black;">Recommended<span class="requ">*</span></label>
                                    <textarea class="form-control" rows="1" cols="40" id="Sec_Recommended_View" name="Sec_Recommended_View" required minlen="1"></textarea>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="col-md-12">
                                    <label for="example-search-input" class="form-model" style="color:black;">File Upload<span class="requ">*</span></label>
                                    <input id="file0" class="form-control file InputPhoto" onchange="uploadFiles('file0','UploadPhotos0')" type="file" accept=".jpg,.jpeg,.png,.doc,.docx,.pdf">
                                    <input type="hidden" value="" class="UploadPhotos" name="UploadPhotos0" id="UploadPhotos0">
                                </div>
                            </div>
                        </div>
                        <div class="">
                            <div style="text-align:center;">
                                <button type="button" class="btn btn-success" onclick="Fn_SecActionTaken_Submit()"> Submit</button>
                            </div>
                        </div>
                    </div>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div>
    </form>
    <div class="row">
        <div class="col-lg-10"> <h5 class="card-title">Action Taken Description</h5></div>
        @if (LoginClass.Role_Id == "5")
        {
            <div class="col-lg-2">
                <button type="button" class="btn btn-success waves-effect waves-light" style="float: right;" data-bs-toggle="modal" id="btnadd" data-bs-target=".add-new"><i class="mdi mdi-plus me-1"></i> Add </button>
            </div>
        }
        else
        {
            <div class="col-lg-2" style="padding:13px"></div>
        }
        <br />
    </div>
    <table id="tblActions_Taken_Description" class="table table-striped dt-responsive nowrap tblround" style="width:100%">
        <thead>
            <tr>
                <th style="width:33%">Action Description</th>
                <th>Comment</th>
                <th>Recommended</th>
                <th style="width:12%">Reported By</th>
