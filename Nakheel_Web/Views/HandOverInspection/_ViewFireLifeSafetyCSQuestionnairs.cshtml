<script>
    ApplySelect2("#Insp_Category");
    function ApplySelect2(id) {
        $(id).select2({
            placeholder: "--Select--",
            theme: 'bootstrap-5',
        });
    }
    var Qns_Action_Check = $(".Qns_Action").val();
    if (Qns_Action_Check == "Yes" || Qns_Action_Check == "NA") {
        $(".Details_of_EvidenceCS").attr('disabled', 'disabled');
        $(".Insp_RiskCS").attr('disabled', 'disabled');
        $(".Risk_DescriptionCS").attr('disabled', 'disabled');
        //$(".QnDescriptionCS").attr('disabled', 'disabled');
        $(".Insp_CategoryCS").attr('disabled', 'disabled');
        $(".Sub_CategoryCS").attr('disabled', 'disabled');
    }
    function Fn_ActionCheck(rowval, status) {
    if (status == "Yes" || status == "NA") {
            $("#Details_of_EvidenceCS_" + rowval).val('');
            $("#Insp_RiskCS_" + rowval).prop('selectedIndex', 0);
            $("#Risk_DescriptionCS_" + rowval).val('');
            $("#QnDescriptionCS_" + rowval).val('');
            $("#Insp_CategoryCS_" + rowval).prop('selectedIndex', 0);
            $("#Sub_CategoryCS_" + rowval).prop('selectedIndex', 0);
            $("#Details_of_EvidenceCS_" + rowval).attr('disabled', 'disabled');
            $("#Insp_RiskCS_" + rowval).attr('disabled', 'disabled');
            $("#Risk_DescriptionCS_" + rowval).attr('disabled', 'disabled');
            $("#Insp_CategoryCS_" + rowval).attr('disabled', 'disabled');
            $("#Sub_CategoryCS_" + rowval).attr('disabled', 'disabled');
            $("#QnDescriptionCS_" + rowval).removeAttr('disabled');
        }
    @*else if (status == "NA") {
            $("#Details_of_EvidenceCS_" + rowval).val('');
            $("#Insp_RiskCS_" + rowval).prop('selectedIndex', 0);
            $("#Risk_DescriptionCS_" + rowval).val('');
            $("#QnDescriptionCS_" + rowval).val('');
            $("#Insp_CategoryCS_" + rowval).prop('selectedIndex', 0);
            $("#Sub_CategoryCS_" + rowval).prop('selectedIndex', 0);
            $("#Details_of_EvidenceCS_" + rowval).attr('disabled', 'disabled');
            $("#Insp_RiskCS_" + rowval).attr('disabled', 'disabled');
            $("#Risk_DescriptionCS_" + rowval).attr('disabled', 'disabled');
            $("#QnDescriptionCS_" + rowval).attr('disabled', 'disabled');
            $("#Insp_CategoryCS_" + rowval).attr('disabled', 'disabled');
            $("#Sub_CategoryCS_" + rowval).attr('disabled', 'disabled');
        }*@
        else {
            $("#Details_of_EvidenceCS_" + rowval).removeAttr('disabled');
            $("#Insp_RiskCS_" + rowval).removeAttr('disabled');
            $("#Risk_DescriptionCS_" + rowval).removeAttr('disabled');
            $("#QnDescriptionCS_" + rowval).removeAttr('disabled');
            $("#Insp_CategoryCS_" + rowval).removeAttr('disabled');
            $("#Sub_CategoryCS_" + rowval).removeAttr('disabled');
        }
    }
    function SubCategoryListCS(Value, rowval) {
        $.post("@Url.Action("Insp_Sub_Category_Master_GetbyId", "Inspection")", { Value: Value }, function (data) {
            $("#Sub_CategoryCS_" + rowval).empty();
            $(data).each(function (i, e) {
                $("#Sub_CategoryCS_" + rowval).append("<option value=" + e.Value + ">" + e.Text + "</option>");
            });
        });
    }
    function Qns_Category_Master(HtmlFind) {
        //debugger;
        var Main_Category = "#Main_Category_" + HtmlFind;
        $.post("@Url.Action("Insp_Category_GetAll", "Inspection")", function (data) {
            $(Main_Category).empty();
            $(Main_Category).append("<option selected value='0'style='text-align:center'  disabled>--Select--</option>");
            $(data).each(function (i, e) {
                $(Main_Category).append("<option value=" + e.Insp_Category_Id + ">" + e.Insp_Category_Name + "</option>");
            });
        });
    }
    function Qns_Sub_Category(Value, HtmlFind) {
        //debugger;
        var Main_SubCategory = "#Main_SubCategory_" + HtmlFind;
        $.post("@Url.Action("Insp_Sub_Category_Master_GetbyId", "Inspection")", { Value: Value }, function (data) {
            $(Main_SubCategory).empty();
            $(data).each(function (i, e) {
                $(Main_SubCategory).append("<option value=" + e.Value + ">" + e.Text + "</option>");
            });
        });
    }
</script>
