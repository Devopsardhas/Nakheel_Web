@model Nakheel_Web.Models.HandOverInsMaster.M_Insp_FireLifeSafety_Model;
@if (Model!.Status == "3")
{
    <form id="F_FireLifeSafety_ChkList_View" action="#" method="post" class="row NotValid" novalidate="novalidate">
        <div class="row">
            <div class="col-12">
                <!-- Question Details -->
                <div class="col-xxl-12 mb-5 mb-xxl-0">
