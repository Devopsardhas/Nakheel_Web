        var Zone_Id = $('#EZone_Id').val();
        var Community_Id = $('#ECommunity_Id').val();
        if (Role_Name == "Service_Provider") {
            EMP_DRP_DWN(Role_Name, Input_Id, Zone_Id, Community_Id);
