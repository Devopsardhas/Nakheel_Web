    function Multiple_Reject_CA_Add() {
        debugger
        var data_Arr = [];
        let valid = $("#FM_Insp_SoftService_TCOE_WalkIn").valid();
        if (!valid) {
            $(window).scrollTop(0);
            return false;
        }
        $('#tbl_Insp_Landscape_Qns tbody > tr').each(function () {
            var varCheckbox_Id = $(this).closest('tr').find("input[type=checkbox]").prop('checked');
            if (varCheckbox_Id == true) {
                var varReject_Reason_Chk = $(this).closest('tr').find('.Reject_Reason_Chk').val();
                var varLandscape_Qns_Id = $(this).closest('tr').find('.Landscape_Qns_Id').val();

                //if(varReject_Reason_Chk == "" || varReject_Reason_Chk == null){
                //    $(this).closest('tr').find('.Reject_Reason_Chk').focus();
                //    return false;
                //}

                var data = {};
                data.Reject_Reason = varReject_Reason_Chk;
                data.Landscape_Qns_Id = varLandscape_Qns_Id;
                data.Remarks = "Reject";
                data_Arr.push(data);
            }
        });
        if (data_Arr.length == "0") {
            toastr.error("Please select at least one checklist", "Error");
            $(window).scrollTop(0);
            return false;
        }



        var obj = {
            Insp_Land_CA_Reject_List: data_Arr,
