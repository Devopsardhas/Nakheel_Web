<script>
    $(document).ready(function () {
        var dtToday = new Date();
        var month = dtToday.getMonth() + 1;
        var day = dtToday.getDate();
        var year = dtToday.getFullYear();
        if (month < 10)
            month = '0' + month.toString();
        if (day < 10)
            day = '0' + day.toString();
        var minDate = year + '-' + month + '-' + day;
        minDate_valid = minDate;
        $('.Target_Date').attr('min', minDate);
        ApplySelect2(".Drop_Search");
    });
    function ApplySelect2(id) {
        $(id).select2({
            placeholder: "--Select--",
            theme: 'bootstrap-5',
        });
    }
    function Fn_SoftService_Qns_CA_Submit() {
        debugger
        let valid = $("#FM_Insp_SoftService_CA_WalkIn").valid();
        if (!valid) {
            $(window).scrollTop(0);
            toastr.error("Please fill all mandatory fields", "Error");
            ApplySelect2(".Drop_Search");
            return false;
        }

        var All_Zone_Emp_List = [];
        var varCompany_Id = $("#Company_Id").val();
        var var_Zone_Emp_Id = $("#All_Zone_Emp_Id").val();
        $(var_Zone_Emp_Id).each(function (i, b) {
            var item = {};
            item.Value = b,
                All_Zone_Emp_List.push(item);
        });

        var Obj = {
            Insp_Request_Id: $("#Insp_Request_Id").val(),
            Company_Id: varCompany_Id,
            Insp_landscap_Zone_Emp_List: All_Zone_Emp_List,
        };
        $.ajax({
            url: "/Inspection/Insp_SoftService_CA_Add",
            type: "POST",
            cache: false,
            data: JSON.stringify(Obj),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                if (data == "200") {
                    Swal.fire({
                        position: 'top-end',
                        icon: 'success',
                        title: 'Added Successfully',
                        showConfirmButton: false,
                        timer: 1500
                    }).then(function () {
                        window.location.reload();
                    });
                }
                else {
                    Swal.fire({
                        position: 'top-end',
                        icon: 'Error',
                        title: 'Error',
                        showConfirmButton: false,
                        timer: 1500
                    });
                }
            },
        });
    }
    function ClosureQnsUploadFiles(inputId, UploadPhotoId) {
        var input = document.getElementById(inputId);
        var files = input.files;
        var formData = new FormData();

        if (files.length <= 5) {

            for (var i = 0; i != files.length; i++) {
                formData.append("files", files[i]);
            }

            var _url = '@Url.Action("Upload_Picture", "Inspection")';

            $.ajax({
                url: _url,
                data: formData,
                processData: false,
                contentType: false,
                type: "POST",
                success: function (data) {
                    debugger
                    if (data == "[\"Invalid Format\"]") {
                        $("#" + inputId).val("");
                        toastr["error"]("Invalid Format");
                    } else {
                        $(".Div_Leader_UploadPicture").hide();
                        $("#" + UploadPhotoId).val("");
                        $("#" + UploadPhotoId).val(data);
                    }
                }
            });
        }

        else {
            $("#" + inputId).val("");
            $("#" + UploadPhotoId).val("");
            toastr["error"]("Maximum Allowed 5 Files");
        }
    }


    function FnActionClosureSubmit(id) {
        var closue_desc = "#Closure_Description_" + id;
        var hdn_Closure_file_paths = "#hdn_Closure_Qns_file_Value_" + id;
        var Landscape_Qns_Id = "#Landscape_Qns_Id_" + id;
        let valid = $("#FM_Insp_SoftService_AC_WalkIn").valid();
        if (!valid) {
            $(closue_desc).focus()
            return false;
        }
        var hdn_Closure_file_path = $(hdn_Closure_file_paths).val();
        var Closure_File_Path = "";

        if (hdn_Closure_file_path != "" && hdn_Closure_file_path != null) {
            var val_UploadPhotos = hdn_Closure_file_path.split(',');
            var i;
            for (i = 0; i < val_UploadPhotos.length; i++) {
                var x = val_UploadPhotos[i].replace(/[\(\)\[\]{}'"]/g, "");
                if (x != "/") {
                    Closure_File_Path = x;
                }
            }
        }

        var Obj = {
            Landscape_Qns_Id: $(Landscape_Qns_Id).val(),
            Closure_Description: $(closue_desc).val(),
            Closure_File_Path: Closure_File_Path,
        };
        $.ajax({
            url: "/Inspection/Insp_SoftService_CA_Closue_Add",
            type: "POST",
            cache: false,
            data: JSON.stringify(Obj),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                if (data.STATUS_CODE == "200") {
                    debugger
                    Swal.fire({
                        position: 'top-end',
                        icon: 'success',
                        title: 'Added Successfully',
                        showConfirmButton: false,
                        timer: 1500
                    }).then(function () {
                        if (data.Return_1 == 0) {
                            window.location.reload();
                        } else {
                            Landscape_Edit(data.Return_1);
                            //var url = "/Inspection/Insp_Safety_Violation_Temp?id=" + data.Return_2;
                            //window.location.href = url;
                        }
                    });
                }
                else {
                    Swal.fire({
                        position: 'top-end',
                        icon: 'Error',
                        title: 'Error',
                        showConfirmButton: false,
                        timer: 1500
                    });
                }
            },
        });
    }

    function L_CA_Action_Approve(id, Name, Insp_Request_Id) {
        debugger
        var Obj = {
            Landscape_Qns_Id: id,
            Remarks: Name,
            Insp_Request_Id: Insp_Request_Id,
