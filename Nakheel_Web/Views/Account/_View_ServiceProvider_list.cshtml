@model IReadOnlyCollection<Nakheel_Web.Models.AccountsMaster.ServiceProviderSignUp>;

@if (Model != null)
{
    <h5 style="color:red">Note : If Add More Contractor or Submit the Process End</h5>
    <div class="card-body">
        <table id="MainTable" class="table table-striped dt-responsive  tblround" style="width:100%">
            <thead>
                <tr class="">
                    <th style="text-align:left;"><i class="mdi mdi-table-key"></i>&nbsp;SignUp Id</th>
                    <th style="text-align:left;"><i class="bx bx-notepad"></i>&nbsp;Company Name</th>
                    <th style="text-align:left;"><i class="bx bx-notepad"></i>&nbsp;Manager Incharge</th>
                    <th style="text-align:left;"><i class="bx bxs-notepad"></i>&nbsp;Mobile No</th>
                    <th style="text-align:left;"><i class="bx bxs-notepad"></i>&nbsp;Zone</th>
                    <th style="text-align:left;"><i class="bx bxs-notepad"></i>&nbsp;Community</th>
                    <th style="text-align:left;"><i class="bx bxs-notepad"></i>&nbsp;Scope of Work</th>
                   @* <th style="text-align:left;"><i class="bx bxs-zap"></i>&nbsp;Action</th>*@
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        
                        <tr class="clstr">
                            <td align="left" valign="middle">@item.Unique_Id</td>
                            <td align="left" valign="middle">@item.Company_Name</td>
                            <td align="left" valign="middle">@item.Manager_Incharge</td>
                            <td align="left" valign="middle">@item.Mobile_No</td>
                            <td align="left" valign="middle">@item.Zone_Id</td>
                             <td align="left" valign="middle">@item.Community_Id</td>
                            <td align="left" valign="middle">@item.W_Scope_of_work</td>
                           @* <td align="left">
                                <div class="d-flex gap-3">
                                    <a onclick="Fn_Delete(@item.SignUp_Id)" href="javascript:void(0);" title="" class="text-info" data-bs-original-title="Delete" aria-label="Delete"><i class="mdi mdi-delete font-size-18"></i></a>
                                </div>
                            </td>*@
                        </tr>
                    }
                }
            </tbody>
        </table>

        <div class="row">
            <div class="col-md-12" align="center">
                <button type="button" class="btn btn-success waves-effect waves-light" id="Update_Sign_Submit" onclick="Fn_ServiceProviderSumbit()">
                    <i class="bx bx-check-double font-size-16 align-middle me-2"></i>Submit
                </button>
                <button type="button" class="btn btn-danger  waves-effect waves-light">
                    <i class="bx bx-check-double font-size-16 align-middle me-2"></i>Cancel
                </button>
            </div>
        </div>
    </div>
}